<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:GUItulator.ViewModels;assembly=GUItulator"
        Icon="resm:GUItulator.Assets.avalonia-logo.ico"
        Title="NametableWindow"
        Height="480"
        Width="512">

    <Grid>
        <Image VerticalAlignment="Top" HorizontalAlignment="Left" Source="{Binding Nametables[0]}"
               Stretch="Uniform" Width="256" Height="240"></Image>
        <Image VerticalAlignment="Top" HorizontalAlignment="Right" Source="{Binding Nametables[1]}"
               Stretch="Uniform" Width="256" Height="240"></Image>
        <Image VerticalAlignment="Bottom" HorizontalAlignment="Left" Source="{Binding Nametables[2]}"
               Stretch="Uniform" Width="256" Height="240"></Image>
        <Image VerticalAlignment="Bottom" HorizontalAlignment="Right" Source="{Binding Nametables[3]}"
               Stretch="Uniform" Width="256" Height="240"></Image>
    </Grid>
</Window>
