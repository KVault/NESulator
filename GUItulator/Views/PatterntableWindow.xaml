<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="clr-namespace:GUItulator.ViewModels;assembly=GUItulator"
        Icon="resm:GUItulator.Assets.avalonia-logo.ico"
        Title="Patterntable viewer"
        Width="512"
        Height="256">

        <Grid>
                <Image HorizontalAlignment="Left" Source="{Binding LeftPatterntable}" Stretch="Uniform"></Image>
                <Image HorizontalAlignment="Right" Source="{Binding RightPatterntable}" Stretch="Uniform"></Image>
        </Grid>
</Window>
